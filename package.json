{
  "name": "@laura-bergoens/nostraint",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && NODE_ENV=production tsc && ./scripts/build/remove_tracelog_calls.sh",
    "clean": "rimraf ./node_modules",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "npm run lint -- --fix",
    "perf": "npm run build && ls -d build/scripts/performance/* | NODE_ENV=production xargs -n1 node",
    "start": "npm run build && node build/index.js",
    "start:dev": "npx nodemon",
    "test": "NODE_ENV=test ts-mocha --exit --recursive --reporter=${MOCHA_REPORTER:-dot} -p ./tsconfig.json \\\"tests/**/*.spec.ts\\\"",
    "test:watch": "NODE_ENV=test npm run test -- --watch --watch-files tests/**/*.spec.ts,src/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-yml": "^1.4.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "pino": "^8.8.0",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
